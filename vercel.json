{
  "version": 2,
  "name": "ai-ux-audit-tool",
  "builds": [
    { "src": "frontend/next.config.js", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ],
  "functions": {
    "frontend/pages/api/audit.ts": {
      "maxDuration": 30
    },
    "frontend/pages/api/test.ts": {
      "maxDuration": 10  
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}