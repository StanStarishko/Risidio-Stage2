{
  "version": 2,
  "name": "ai-ux-audit-tool",
  "builds": [
    { 
      "src": "frontend/package.json", 
      "use": "@vercel/next",
      "config": {
        "outputDirectory": "frontend/.next"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ],
  "functions": {
    "frontend/pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}