{
  "name": "ai-ux-audit-tool",
  "version": "1.2.0",
  "description": "AI-powered UX audit tool with Web3 integration for Lunim Studio",
  "private": true,
  "workspaces": ["frontend", "backend"],
  "engines": { "node": ">=18.18.0", "npm": ">=8.0.0" },
  "scripts": {
    "dev": "concurrently -n backend,frontend -c magenta,cyan \"npm --prefix backend run dev\" \"npm --prefix frontend run dev\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "build": "npm --prefix frontend run build",
    "start": "npm --prefix frontend run start",
    "test": "npm --prefix frontend run test || echo \"no tests\"",
    "lint": "npm --prefix frontend run lint || echo \"no lint\"",
    "clean": "rm -rf frontend/.next backend/dist node_modules/*/node_modules api/.vercel",
    "install:all": "npm ci && npm --prefix frontend ci && npm --prefix backend ci"
  },
  "repository": { "type": "git", "url": "https://github.com/StanStarishko/Portfolio/" },
  "keywords": ["ux-audit", "ai", "accessibility", "web3", "blockchain", "ui-analysis", "user-experience"],
  "author": { "name": "Stanislav Starishko", "email": "slavkost73@gmail.com", "url": "https://www.linkedin.com/in/starishko/" },
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "node-fetch": "3.3.2",
    "openai": "4.55.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0"
  }
}
